{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": []
    },
    "deploy": {
      "dependsOn": ["build", "test"]
    },
    "dev": {
      "cache": false
    },
    "example#dev": {
      "cache": false,
      "dependsOn": ["v-perfect-signature#build"]
    },
    "clean": {
      "cache": false
    }
  }
}
